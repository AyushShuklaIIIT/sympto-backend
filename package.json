{
  "name": "sympto-backend",
  "version": "1.0.0",
  "description": "Backend API for Sympto health assessment platform",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate-keys": "node scripts/generateKeys.js",
    "crypto:scan-users": "node scripts/scanUndecryptableUsers.js",
    "crypto:cleanup-users": "node scripts/scanUndecryptableUsers.js --delete"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsdom": "^27.4.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "mongoose-encryption": "^2.1.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@types/bcryptjs": "^2.4.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "health",
    "assessment",
    "api",
    "nodejs",
    "express"
  ],
